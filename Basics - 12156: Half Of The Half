#include <stdio.h>
#include <string.h>

int main()
{
    char s[201];
    int t, i;

    scanf("%d", &t);

    while(t>=0) {
        gets(s);
        for(i=0; i<(strlen(s)/2); i+=2)
            putchar(s[i]);
        putchar('\n');
    }
    return 0;
}
